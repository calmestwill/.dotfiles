URxvt*font: xft:Sauce Code Pro Nerd Font Mono:size=10
URxvt.scrollBar: false
URxvt.perl-ext-common: keyboard-select,tabbedex
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.cursorBlink: 1

! tab extension
URxvt.tabbed.new-button: false
URxvt.tabbed.tab-numbers: false
URxvt.tabbed.title: false

! special
!URxvt*.background: rgba:1f00/1f00/1f00/e000
!URxvt*.borderColor: rgba:1f00/1f00/1f00/c300
URxvt*.borderColor: #232c31
URxvt*.depth: 32

! special
*.foreground:   #9ea7a6
*.background:   #232c31
*.cursorColor:  #9ea7a6

! black
*.color0:       #232c31
*.color8:       #3f4944

! red
*.color1:       #2a5491
*.color9:       #2a5491

! green
*.color2:       #237986
*.color10:      #237986

! yellow
*.color3:       #a03b1e
*.color11:      #a03b1e

! blue
*.color4:       #484d79
*.color12:      #484d79

! magenta
*.color5:       #c59820
*.color13:      #c59820

! cyan
*.color6:       #b02f30
*.color14:      #b02f30

! white
*.color7:       #9ea7a6
*.color15:      #b5d8f6



